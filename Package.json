{
  "name": "ai-nexus",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "NODE_ENV=development tsx watch src/server/index.ts",
    "dev:frontend": "vite",
    "build": "vite build && npm run build:backend",
    "build:backend": "esbuild src/server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist/server",
    "start": "NODE_ENV=production node dist/server/index.js",
    "preview": "vite preview",
    "check": "tsc --noEmit",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit pull",
    "db:generate": "drizzle-kit generate",
    "lint": "eslint src --ext ts,tsx",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "format": "prettier --write src"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@neondatabase/serverless": "^0.10.4",
    "@radix-ui/react-accordion": "^1.2.4",
    "@radix-ui/react-alert-dialog": "^1.1.7",
    "@radix-ui/react-aspect-ratio": "^1.1.3",
    "@radix-ui/react-avatar": "^1.1.4",
    "@radix-ui/react-checkbox": "^1.1.5",
    "@radix-ui/react-collapsible": "^1.1.4",
    "@radix-ui/react-context-menu": "^2.2.7",
    "@radix-ui/react-dialog": "^1.1.7",
    "@radix-ui/react-dropdown-menu": "^2.1.7",
    "@radix-ui/react-hover-card": "^1.1.7",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-menubar": "^1.1.7",
    "@radix-ui/react-popover": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.4",
    "@radix-ui/react-select": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.3",
    "@radix-ui/react-slider": "^1.2.2",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.4",
    "@radix-ui/react-toast": "^1.2.5",
    "@radix-ui/react-toggle": "^1.1.3",
    "@radix-ui/react-toggle-group": "^1.1.4",
    "@radix-ui/react-tooltip": "^1.2.2",
    "@tanstack/react-query": "^5.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "connect-pg-simple": "^9.0.1",
    "drizzle-orm": "^0.36.2",
    "express": "^4.20.1",
    "express-session": "^1.18.1",
    "framer-motion": "^11.5.13",
    "lucide-react": "^0.409.0",
    "next-themes": "^0.2.1",
    "passport": "^0.7.0",
    "pg": "^8.12.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.2",
    "react-router-dom": "^6.24.3",
    "tailwind-merge": "^2.4.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/connect-pg-simple": "^7.0.6",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.8",
    "@types/node": "^20.12.0",
    "@types/passport": "^1.0.16",
    "@types/pg": "^8.11.6",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.0",
    "drizzle-kit": "^0.23.1",
    "esbuild": "^0.21.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.3",
    "tsx": "^4.7.0",
    "typescript": "^5.4.5",
    "vite": "^5.1.6"
  },
  "engines": {
    "node": ">=20.11.0",
    "npm": ">=9.0.0"
  }
}
